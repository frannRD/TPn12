#include<iostream>
using namespace std;
class Punto{
int x;
int y;
public:
Punto();
Punto(int cx,int cy);
void imprimir();
};
Punto::Punto(){
x=0;
y=0;
}
Punto::Punto(int cx,int cy){
x=cx;
y=cy;
}
void Punto::imprimir(){
cout<<"("<<x<<","<<y<<")"<<endl;
}
int main(){
Punto p1;
Punto p2(5,9);
cout<<"Punto 1: ";
p1.imprimir();
cout<<"Punto 2: ";
p2.imprimir();
return 0;
}


