#include<iostream>
using namespace std;
class Vector5{
int v[5];
public:
Vector5();
void imprimir();
void imprimir(int hasta);
void imprimir(int desde,int hasta);
};
Vector5::Vector5(){
cout<<"Ingrese 5 numeros:"<<endl;
for(int i=0;i<5;i++){
cin>>v[i];
}
}
void Vector5::imprimir(){
for(int i=0;i<5;i++){
cout<<v[i]<<" ";
}
cout<<endl;
}
void Vector5::imprimir(int hasta){
if(hasta<0)hasta=0;
if(hasta>4)hasta=4;
for(int i=0;i<=hasta;i++){
cout<<v[i]<<" ";
}
cout<<endl;
}
void Vector5::imprimir(int desde,int hasta){
if(desde<0)desde=0;
if(hasta>4)hasta=4;
if(desde>hasta)return;
for(int i=desde;i<=hasta;i++){
cout<<v[i]<<" ";
}
cout<<endl;
}
int main(){
Vector5 vec;
cout<<"vector completo:"<<endl;
vec.imprimir();
cout<<"Desde posicion 0 hasta la posicion 2:"<<endl;
vec.imprimir(2);
cout<<"Desde posicion 1 hasta la 3:"<<endl;
vec.imprimir(1,3);
return 0;
